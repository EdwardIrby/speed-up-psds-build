{
  "name": "@pluralsight/ps-design-system-form",
  "version": "8.0.20",
  "description": "Design system components for form layout",
  "license": "Apache-2.0",
  "repository": "pluralsight/design-system",
  "author": "jaketrent",
  "keywords": [
    "pluralsight",
    "design system",
    "form ui"
  ],
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "scripts": {
    "build": "run-s build:cjs build:esm build:css",
    "build:cjs": "tsc --project tsconfig.build.json --module commonjs --target es5 --outDir dist/cjs",
    "build:css": "build-css --useGlamor -i dist/cjs/css/index.js",
    "build:esm": "tsc --project tsconfig.build.json --module es2015 --target es5 --outDir dist/esm",
    "build:watch": "yarn build:esm --watch",
    "storybook": "start-storybook -p 6006",
    "test": "tsc --noEmit --project ./tsconfig.json && jest",
    "test:watch": "yarn test --watchAll"
  },
  "style": "dist/index.css",
  "types": "dist/esm/index.d.ts",
  "dependencies": {
    "@pluralsight/ps-design-system-core": "^6.6.2",
    "@pluralsight/ps-design-system-util": "^6.0.8"
  },
  "peerDependencies": {
    "@pluralsight/ps-design-system-normalize": "^4.0.0",
    "@pluralsight/ps-design-system-theme": "^6.0.0",
    "glamor": "^2.20.40",
    "react": "^16.8.6"
  },
  "devDependencies": {
    "@pluralsight/ps-design-system-actionmenu": "^15.4.4",
    "@pluralsight/ps-design-system-build": "^2.0.2",
    "@pluralsight/ps-design-system-button": "^19.0.16",
    "@pluralsight/ps-design-system-checkbox": "^6.2.18",
    "@pluralsight/ps-design-system-datepicker": "^10.0.2",
    "@pluralsight/ps-design-system-dropdown": "^10.0.5",
    "@pluralsight/ps-design-system-radio": "^7.2.17",
    "@pluralsight/ps-design-system-storybook-preset": "^0.2.17",
    "@pluralsight/ps-design-system-switch": "^6.2.18",
    "@pluralsight/ps-design-system-tag": "^7.2.9",
    "@pluralsight/ps-design-system-text": "^17.0.4",
    "@pluralsight/ps-design-system-textarea": "^7.3.7",
    "@pluralsight/ps-design-system-textinput": "^9.0.17",
    "@pluralsight/ps-design-system-theme": "^6.0.13"
  }
}
