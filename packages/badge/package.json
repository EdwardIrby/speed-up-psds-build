{
  "name": "@pluralsight/ps-design-system-badge",
  "version": "11.1.9",
  "description": "Design System component for badge ui",
  "license": "Apache-2.0",
  "repository": "pluralsight/design-system",
  "author": "jaketrent",
  "keywords": [],
  "main": "dist/esm/index.mjs",
  "module": "dist/esm/index.mjs",
  "type": "module",
  "sideEffects": false,
  "scripts": {
    "build": "run-s build:esm build:css",
    "build:css": "build-css --useGlamor -i dist/esm/css/index.mjs",
    "build:esm": "run-s compile:esm rewrite:esm",
    "compile:esm": "tsc --project tsconfig.build.json --module esnext --target es5 --outDir dist/esm",
    "rewrite:esm": "tsc-esm --project tsconfig.build.json",
    "build:watch": "yarn build:js --watch",
    "storybook": "start-storybook -p 6006",
    "test": "jest",
    "test:watch": "yarn test --watchAll"
  },
  "style": "dist/css/index.css",
  "dependencies": {
    "@pluralsight/ps-design-system-core": "^7.0.0",
    "@pluralsight/ps-design-system-util": "^8.0.0"
  },
  "peerDependencies": {
    "@pluralsight/ps-design-system-normalize": "^4.0.0",
    "@pluralsight/ps-design-system-theme": "^7.0.0",
    "glamor": "^2.20.40",
    "react": "^17.0.1"
  },
  "devDependencies": {
    "@pluralsight/ps-design-system-build": "^2.1.0",
    "@pluralsight/ps-design-system-storybook-preset": "^0.4.8",
    "@pluralsight/ps-design-system-theme": "^7.1.8"
  }
}
