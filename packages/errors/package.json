{
  "name": "@pluralsight/ps-design-system-errors",
  "version": "11.0.6",
  "description": "Design system components for errors",
  "license": "Apache-2.0",
  "repository": "pluralsight/design-system",
  "author": "jaketrent",
  "keywords": [],
  "main": "dist/esm/index.mjs",
  "module": "dist/esm/index.mjs",
  "scripts": {
    "build": "run-s build:svg build:esm build:types build:css",
    "build:css": "build-css --useGlamor -i dist/esm/css/index.mjs",
    "build:esm": "run-s compile:esm rewrite:esm rewriteillustrations:esm",
    "build:svg": "run-s convert:svg removeindex:svg",
    "build:types": "tsc --project tsconfig.build.json --module es2015 --target es5 --outDir dist/esm",
    "build:watch": "yarn build:esm --watch",
    "compile:esm": "tsc --project tsconfig.build.json --module es2015 --target es5 --outDir dist/esm",
    "convert:svg": "svgr --typescript --filename-case kebab --ext \"dist.tsx\" -d src/react/illustrations src/svg",
    "removeindex:svg": "rimraf src/react/illustrations/index.dist.tsx",
    "rewrite:esm": "tsc-esm --project tsconfig.build.json",
    "rewriteillustrations:esm": "node build/rewrite-illustrations-index.js",
    "storybook": "start-storybook -p 6006",
    "test": "tsc --noEmit --project ./tsconfig.json && jest",
    "test:watch": "yarn test --watchAll"
  },
  "sideEffects": false,
  "style": "dist/index.css",
  "type": "module",
  "types": "dist/esm/index.d.ts",
  "dependencies": {
    "@pluralsight/ps-design-system-button": "^21.0.6",
    "@pluralsight/ps-design-system-core": "^6.9.3",
    "@pluralsight/ps-design-system-icon": "^21.0.6",
    "@pluralsight/ps-design-system-text": "^18.2.2",
    "@pluralsight/ps-design-system-util": "^7.3.0",
    "camelize": "^1.0.0"
  },
  "peerDependencies": {
    "@pluralsight/ps-design-system-normalize": "^4.0.0",
    "@pluralsight/ps-design-system-theme": "^7.0.0",
    "glamor": "^2.20.40",
    "react": "^17.0.1"
  },
  "devDependencies": {
    "@pluralsight/ps-design-system-build": "^2.1.0",
    "@pluralsight/ps-design-system-normalize": "^4.4.0",
    "@pluralsight/ps-design-system-storybook-preset": "^0.4.6",
    "@pluralsight/ps-design-system-theme": "^7.1.6"
  }
}
