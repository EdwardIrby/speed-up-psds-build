{
  "name": "@pluralsight/ps-design-system-link",
  "version": "11.1.6",
  "description": "Design System component for link/anchor ui",
  "license": "Apache-2.0",
  "repository": "pluralsight/design-system",
  "author": "jaketrent",
  "keywords": [],
  "main": "dist/index.js",
  "module": "dist/index.js",
  "scripts": {
    "build": "run-s build:esm build:css",
    "build:css": "postcss src/css/index.css -o dist/css/index.css && cp dist/css/index.css dist/css/index.scss",
    "build:esm": "tsc --project tsconfig.build.json --module es2015 --target es5 --outDir dist",
    "build:watch": "yarn build:esm --watch",
    "storybook": "start-storybook -p 6006 -s ./dist",
    "test": "jest",
    "test:watch": "yarn test --watchAll"
  },
  "typings": "./react.d.ts",
  "dependencies": {
    "@pluralsight/ps-design-system-core": "^6.9.3",
    "@pluralsight/ps-design-system-util": "^7.3.0"
  },
  "peerDependencies": {
    "@pluralsight/ps-design-system-normalize": "^4.0.0",
    "@pluralsight/ps-design-system-theme": "^7.0.0",
    "glamor": "^2.20.40",
    "react": "^17.0.1"
  },
  "devDependencies": {
    "@pluralsight/ps-design-system-build": "^2.1.0",
    "@pluralsight/ps-design-system-storybook-preset": "^0.4.6",
    "@pluralsight/ps-design-system-theme": "^7.1.6"
  }
}