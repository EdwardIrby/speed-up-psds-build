{
  "name": "@pluralsight/ps-design-system-storybook-addon-theme",
  "version": "6.0.8",
  "description": "Storybook addon for Theme switching",
  "license": "Apache-2.0",
  "repository": "pluralsight/design-system",
  "author": "jaketrent",
  "keywords": [
    "react-storybook"
  ],
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "scripts": {
    "build": "run-s build:js build:cjs",
    "build:cjs": "babel --root-mode upward src --out-dir dist/cjs",
    "build:js": "cross-env ESM=true babel --root-mode upward src --out-dir dist/esm",
    "build:watch": "npm run build:js -- --watch",
    "prepublish": "npm run build"
  },
  "dependencies": {
    "@pluralsight/ps-design-system-core": "^6.2.1",
    "@storybook/addons": "^5.2.5",
    "@storybook/core-events": "^5.2.5"
  },
  "peerDependencies": {
    "@pluralsight/ps-design-system-theme": "^4.0.0",
    "glamor": "^2.20.40",
    "react": "^16.8.6"
  },
  "devDependencies": {
    "@pluralsight/ps-design-system-theme": "^4.0.11"
  }
}
