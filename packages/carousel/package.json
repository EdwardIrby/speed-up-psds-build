{
  "name": "@pluralsight/ps-design-system-carousel",
  "version": "7.0.2",
  "description": "Carousel UI Component for the Pluralsight Design System",
  "license": "Apache-2.0",
  "repository": "pluralsight/design-system",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "run-s build:js build:cjs build:css",
    "build:cjs": "babel --root-mode upward src --out-dir dist/cjs",
    "build:css": "build-css --useGlamor -i dist/cjs/css/index.js",
    "build:js": "cross-env ESM=true babel --root-mode upward src --out-dir dist/esm",
    "build:watch": "npm run build:js -- --watch",
    "prepublish": "npm run build",
    "storybook": "start-storybook -p 6006",
    "test": "jest",
    "test:watch": "npm run test -- --watchAll"
  },
  "gitHead": "ea4595a4bff6a08255102a0321bb641dc4d25913",
  "dependencies": {
    "@pluralsight/ps-design-system-core": "^5.5.1",
    "@pluralsight/ps-design-system-filter-react-props": "^1.1.16",
    "@pluralsight/ps-design-system-halo": "^6.1.2",
    "@pluralsight/ps-design-system-icon": "^17.1.2",
    "resize-observer-polyfill": "^1.5.1"
  },
  "peerDependencies": {
    "@pluralsight/ps-design-system-normalize": "^3.1.7",
    "@pluralsight/ps-design-system-theme": "^4.0.0",
    "glamor": "^2.x.x",
    "react": "^16.8.6"
  },
  "devDependencies": {
    "@pluralsight/ps-design-system-actionmenu": "^9.1.3",
    "@pluralsight/ps-design-system-build": "^1.11.10",
    "@pluralsight/ps-design-system-card": "^13.1.2",
    "@pluralsight/ps-design-system-position": "^2.0.24",
    "@pluralsight/ps-design-system-storybook-addon-center": "^2.1.9",
    "@pluralsight/ps-design-system-storybook-addon-chaos": "^0.0.18",
    "@pluralsight/ps-design-system-storybook-addon-theme": "^5.0.16",
    "@pluralsight/ps-design-system-theme": "^4.0.9"
  }
}
