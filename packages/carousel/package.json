{
  "name": "@pluralsight/ps-design-system-carousel",
  "version": "10.2.2",
  "description": "Carousel UI Component for the Pluralsight Design System",
  "license": "Apache-2.0",
  "repository": "pluralsight/design-system",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "style": "dist/index.css",
  "types": "dist/esm/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "run-s build:svg build:cjs build:esm build:css copy:svg",
    "build:cjs": "tsc --project tsconfig.build.json --module commonjs --target es5 --outDir dist/cjs",
    "build:css": "build-css --useGlamor -i dist/cjs/css/index.js",
    "build:esm": "tsc --project tsconfig.build.json --module es2015 --target es5 --outDir dist/esm",
    "build:svg": "svgr --typescript --filename-case kebab --ext \"dist.tsx\" -d src/react/illustrations src/svg",
    "build:watch": "yarn build:esm -- --watch",
    "storybook": "start-storybook -p 6006",
    "test": "tsc --noEmit --project ./tsconfig.json && jest",
    "test:watch": "yarn test -- --watchAll"
  },
  "gitHead": "7418883d96a76c59a99f86c2b16516ec752bf913",
  "dependencies": {
    "@pluralsight/ps-design-system-core": "^6.5.1",
    "@pluralsight/ps-design-system-filter-react-props": "^1.2.1",
    "@pluralsight/ps-design-system-halo": "^7.1.2",
    "@pluralsight/ps-design-system-icon": "^19.0.0",
    "@pluralsight/ps-design-system-util": "^5.4.2",
    "resize-observer-polyfill": "^1.5.1"
  },
  "peerDependencies": {
    "@pluralsight/ps-design-system-normalize": "^4.0.0",
    "@pluralsight/ps-design-system-theme": "^4.0.0",
    "glamor": "^2.x.x",
    "react": "^16.8.6"
  },
  "devDependencies": {
    "@pluralsight/ps-design-system-actionmenu": "^15.2.2",
    "@pluralsight/ps-design-system-build": "^1.12.1",
    "@pluralsight/ps-design-system-card": "^14.1.2",
    "@pluralsight/ps-design-system-position": "^4.2.2",
    "@pluralsight/ps-design-system-storybook-preset": "^0.2.2",
    "@pluralsight/ps-design-system-theme": "^5.1.2"
  }
}
