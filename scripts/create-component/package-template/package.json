    "@storybook/addon-storyshots": "^3.x.x",
    "@storybook/addons": "^3.x.x",
    "@storybook/react": "^3.x.x",
    "babel-cli": "^6.x.x",
    "babel-preset-env": "^1.x.x",
    "babel-preset-react": "^6.x.x",
    "babel-preset-stage-2": "^6.x.x",
